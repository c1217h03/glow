{
    "manifest_version": 3,
    "name": "Glow",
    "version": "1.0",
    "description": "An extension for Sephora to check if products have pore cloggin ingredients",
    "permissions": ["activeTab"],
    "icons": {
        "16": "images/glow-16.png",
        "32": "images/glow-32.png",
        "48": "images/glow-48.png",
        "128": "images/glow-128.png"
    },
    "action": {
        "default_icon": {
            "16": "images/glow-16.png",
            "32": "images/glow-32.png",
            "48": "images/glow-48.png",
            "128": "images/glow-128.png"
        }
    }
}